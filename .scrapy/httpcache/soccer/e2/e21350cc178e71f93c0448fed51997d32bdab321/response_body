<script>
	        function trim(str){ //删除左右两端的空格
  		   return str.replace(/(^\s*)|(\s*$)/g, "");
 		}
    	function getCookie(name){//获取指定名称的cookie的值 
    	 
			var cookies = document.cookie.split(";"); 
			for(var i = 0;i < cookies.length;i ++){ 
				var cookie = cookies[i].split("="); 
				var c_name=trim(cookie[0]);
				if(c_name==name){
					return cookie[1];
				} 
			} 
		} 
     
 	function load_userinfo(){
	var nickname=getCookie("nickname");
    	if(nickname!=undefined && nickname.length>2){
			nickname=decodeURI(nickname);
			var info="<span>[<a href='#'>"+nickname+"</a>]您好！欢迎来到搜达足球</span><a href='http://www.sodasoccer.com/passport/logout.action'>退出</a>";
			document.getElementById("userinfo").innerHTML=info;
		}else{
			var info="<span>您好！欢迎来到搜达足球</span><a href='http://passport.sodasoccer.com/login.html'>[登录]</a><a href='http://passport.sodasoccer.com/login.html'>[注册]</a>";
			document.getElementById("userinfo").innerHTML=info;
		}
	}
	load_userinfo();
